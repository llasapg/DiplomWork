services:
  diplomasolution:
    build:
      context: /Users/llasapg/Desktop/DiplomWork
      dockerfile: DiplomaSolution/Dockerfile
      labels:
        com.microsoft.created-by: visual-studio
        com.microsoft.visual-studio.project-name: DiplomaSolution
    container_name: DiplomaSolution
    entrypoint: tail -f /dev/null
    environment:
      ASPNETCORE_ENVIRONMENT: Development
      ASPNETCORE_LOGGING__CONSOLE__DISABLECOLORS: "true"
      ASPNETCORE_URLS: https://+:443;http://+:80
      DOTNET_USE_POLLING_FILE_WATCHER: '1'
      NUGET_FALLBACK_PACKAGES: ''
    image: diplomasolution:dev
    labels:
      com.microsoft.visualstudio.debuggee.arguments: ' --additionalProbingPath /root/.nuget/packages  "/app/bin/Debug/netcoreapp3.1/DiplomaSolution.dll"'
      com.microsoft.visualstudio.debuggee.killprogram: /bin/sh -c "if PID=$$(pidof
        dotnet); then kill $$PID; fi"
      com.microsoft.visualstudio.debuggee.program: dotnet
      com.microsoft.visualstudio.debuggee.workingdirectory: /app
    ports:
    - target: 80
    - target: 443
    tty: true
    volumes:
    - /Users/llasapg/Desktop/DiplomWork/DiplomaSolution:/app:rw
    - /Users/llasapg/vsdbg/vs2017u5:/remote_debugger:rw
    - /Users/llasapg/.aspnet/https:/root/.aspnet/https:ro
    - /Users/llasapg/.microsoft/usersecrets:/root/.microsoft/usersecrets:ro
    - /Users/llasapg/.nuget/packages:/root/.nuget/packages:ro
    - /Users/llasapg/Desktop/DiplomWork:/src:rw
version: '3.4'