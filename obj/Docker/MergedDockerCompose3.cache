services:
  diplomasolution:
    build:
      context: /Users/llasapg/Desktop/DiplomWork
      dockerfile: Dockerfile
    environment:
      ASPNETCORE_ENVIRONMENT: Development
      ASPNETCORE_URLS: https://+:443;http://+:80
    image: diplomasolution
    ports:
    - target: 80
    - target: 443
    volumes:
    - /Users/llasapg/.aspnet/https:/root/.aspnet/https:ro
    - /Users/llasapg/.microsoft/usersecrets:/root/.microsoft/usersecrets:ro
  mysql-development:
    environment:
      MYSQL_DATABASE: MedicalDb
      MYSQL_ROOT_PASSWORD: root
    image: mysql:8.0.17
    ports:
    - published: 3308
      target: 3306
version: '3.4'